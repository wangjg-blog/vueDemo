/*index-home*/
/*banner*/
.swiper-container-banner {
    position: relative;
    max-height: 3.9rem;
}

.banner-img {
    width: 100%;
    max-height: 3.9rem;
}

.search-color {
    background: #157fd0;
}

[m-role="UINavigationBar"].indexBar,
.indexBar .UINavigationBarRoom {
    background: none;
}

.indexBar .UINavigationBarRoom.search-col1 {
    background: rgba(21, 127, 208, 0.5);
}

.indexBar .UINavigationBarRoom.search-col2 {
    background: rgba(21, 127, 208, 1);
}

.letter {
    position: relative;
    width: 0.6rem;
    height: 0.6rem;
    border-radius: 50%;
    background: #fff url(images/letter.png) center center no-repeat;
    background-size: 0.4rem 0.3rem;
    display: block;
    margin-right: 0.25rem;
}

.letter-num {
    display: block;
    width: 0.18rem;
    height: 0.18rem;
    background: #ff0000;
    border-radius: 50%;
    position: absolute;
    top: 0.1rem;
    right: 0.05rem;
}

.swiper-touch {
    height: 2.4rem;
    background: #fff;
    position: relative;
}

.icon-h {
    width: 1.1rem;
    height: 1.1rem;
    background: url(images/icon-home.png) no-repeat;
    background-size: 1.1rem 6.6rem;
    display: block;
    margin: 0.1rem;
}

.icon-home-1 {
    background-position: 0rem 0rem
}

.icon-home-2 {
    background-position: 0rem -1.1rem
}

.icon-home-3 {
    background-position: 0rem -2.2rem
}

.icon-home-4 {
    background-position: 0rem -3.3rem
}

.word-hid {
    height: 0.35rem;
    overflow: hidden;
}

/*swiper-huo*/

.swiper-container-huo {
    width: 100%;
    height: 2.4rem;
}

.swiper-container-huo .swiper-slide {
    width: 2.5rem;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    background: #fff;
}

.swiper-container-huo .swiper-slide > div {
    margin-top: -0.3rem;
}

.swiper-container-horizontal > .swiper-scrollbar-huo {
    position: absolute;
    left: 50%;
    bottom: 0.2rem;
    z-index: 50;
    height: 0.06rem;
    width: 0.7rem;
    margin-left: -0.35rem;
    background: #d8d8d8;
}

.swiper-scrollbar-drag {
    height: 100%;
    width: 100%;
    position: relative;
    background: #4da5f3 !important;
    border-radius: 0 !important;
    left: 0;
    top: 0
}

.swiper-scrollbar-cursor-drag {
    cursor: move
}

/*tips-new*/
.tips-new {
    height: 0.9rem;
    background: #fff;
    position: relative;
    padding: 0 0.2rem;
}

.tips-new:after {
    position: absolute;
    top: 0;
    left: 0rem;
    right: 0rem;
    content: ' ';
    width: auto;
    height: 1px;
    border-bottom: 1px solid #e6eaed;
    transform: scaleY(0.5);
!important;
}
/*tips-notice*/
.tips-notice {
    height: 1.3rem;
    background: #FFFAC8;
    position: relative;
    padding: 0 0.2rem;
}

.tips-notice:after {
    position: absolute;
    top: 0;
    left: 0rem;
    right: 0rem;
    content: ' ';
    width: auto;
    height: 1px;
    border-bottom: 1px solid #e6eaed;
    transform: scaleY(0.5);
!important;
}

.new-icon {
    width: 0.3rem;
    height: 0.36rem;
    background: url(images/new-icon.png) no-repeat;
    background-size: 100%;
    display: block;
    margin-right: 0.2rem;
}

/*fund-box*/
.fund-box {
    background: #fff;
    padding-bottom: 0.6rem;
}

.fund-box > div:nth-child(1) {
    margin-top: 0.6rem;
}

.fund-trait > div {
    border: 1px solid #777777;
    height: 0.35rem;
    line-height: 0.35rem;
    border-radius: 5px;
    padding: 0 0.25rem;
}

.fund-trait > div:nth-child(2) {
    margin: 0 0.1rem;
}

.fund-btn {
    width: 5.2rem;
    height: 0.7rem;
    margin: 0.25rem auto 0;
}

.fund-btn > div {
    width: 2.4rem;
    color: #fff;
    font-size: 0.34rem;
    border-radius: 5px;
}

.fund-btn > div:nth-child(1) {
    background: #fd6749;
    margin-right: 0.2rem;

}

.fund-btn > div:nth-child(2) {
    background: #157fd0;
    margin-left: 0.2rem;

}

/*new-pro*/
.new-pro {
    height: auto;
    background: #fff;
    border-bottom: 1px #E6EAED solid;
    padding: 0.2rem 0;
    min-height: 2rem;
}

.new-pro-logo, .group-fund-logo, .theme-fund-logo, .choice-fund-num {
    width: 2.7rem;
}

.new-pro-logo > img {
    height: 1.6rem;
}

/*new-pro-newcomer*/
.new-pro-newcomer {
    height: auto;
    background: #fff;
    /*border-bottom: 1px #E6EAED solid;*/
    padding: 0.2rem 0;
    min-height: 2rem;
}

.group-fund-logo > img {
    height: 1.45rem;
}

.theme-fund-logo > img {
    height: 1.45rem;
}

.pro_left {
    position: relative;
    padding-left: 0.4rem;
}

.pro_left:after {
    position: absolute;
    top: 0.55rem;
    bottom: 0.55rem;
    left: 0rem;
    content: ' ';
    width: 1px;
    height: 1rem;
    border-left: 1px solid #e6eaed;
    transform: scaleX(1);
!important;
}

.pro-border {
    border: 1px solid #777777;
    border-radius: 4px;
    padding: 0.06rem 0.2rem ;
}

.choice-fund, .group-fund, .theme-fund, .news-list {
    background: #fff;
}

.choice-fund-head, .group-fund-head, .theme-fund-head, .news-list-head {
    height: 0.65rem;
    position: relative;
    padding-left: 0.35rem;
    background-color: #fafafa;
}

.choice-fund-head:after, .group-fund-head:after, .theme-fund-head:after, .news-list-head:after {
    position: absolute;
    bottom: 0;
    left: 0rem;
    right: 0rem;
    content: ' ';
    width: auto;
    height: 1px;
    border-bottom: 1px solid #e6eaed;
    transform: scaleY(0.5);
!important;
}

[m-role='UITableView'][m-style='grouped'] > dl.news > dd {
    padding: 0 0.8rem 0 0.35rem;
    min-height: 0.9rem;
    position: relative;
    border: none;
}

.news-tips {
    border: 1px solid #777777;
    border-radius: 4px;
    padding: 0.06rem 0.15rem;
}

.news-word {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

[m-role='UITableView'][m-style='grouped'] > dl.news > dd:after {
    position: absolute;
    bottom: 0;
    left: 0.35rem;
    right: 0.3rem;
    content: ' ';
    width: auto;
    height: 1px;
    border-bottom: 1px dashed #e6eaed;
    transform: scaleY(0.5);
!important;
}

#scrollDiv {
    width: auto;
    height: 25px;
    line-height: 25px;
    overflow: hidden
}

#scrollDiv ul {
    width: 100%;
}

#scrollDiv li {
    height: 25px;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

/*discover*/
.search-color {
    background: #157fd0;
}

.discover-searchbar {
    background: none;
    height: auto;
}

.discover-searchbar .UISearchBarRoom {
    border-bottom: none;
    height: 0.6rem;
    background: none;
    top: 0.1rem;
    left: 1rem;
    right: 1rem;
}

[m-role='cell'].search-box {
    border: 1px solid #fff;
    background-color: #fff;
    line-height: normal;
    border-radius: 0;
    position: relative;
    font-size: 14px;
    height: 0.56rem;
    padding-left: 30px;
}

.discover-searchbar [m-role='cell'].search-box [m-icon='search'] {
    width: 0.35rem;
    height: 0.4rem;
    background: url("images/search-icon.png") center center no-repeat;
    background-size: 100%;
    margin-top: 0.1rem;
    margin-left: 0.1rem;
}

.discover-searchbar [m-role='cell'].search-box {
    border-radius: 8px;
}

.discover-searchbar [m-role='cell'].search-box input {
    background: 0;
    border: 0;
    line-height: normal;
    width: 100%;
    height: 100%;
    white-space: nowrap;
    display: block;
    text-align: left;
    font-size: 0.28rem;
}

[m-role='cell'] [m-icon='search'] {
    position: absolute;
    top: -1px;
    left: 0;
}

.icon-dis {
    background: #fff;
    height: 2.1rem;
}

.icon-d {
    width: 1rem;
    height: 1rem;
    background: url(images/icon-dis.png) no-repeat;
    background-size: 1rem 3.3rem;
    display: block;
}

.icon-dis-1 {
    background-position: 0rem 0rem
}

.icon-dis-2 {
    background-position: 0rem -1.1rem
}

.icon-dis-3 {
    background-position: 0rem -2.2rem
}

.choice-fund-dis {
    position: relative;
}

.choice-fund-dis:after {
    position: absolute;
    bottom: 0;
    left: 0rem;
    right: 0rem;
    content: ' ';
    width: auto;
    height: 1px;
    border-bottom: 1px solid #e6eaed;
    transform: scaleY(0.5);
!important;
}

.more-infor {
    position: absolute;
    height: 0.65rem;
    line-height: 0.65rem;
    right: 0.3rem;
    color: #999;
    font-size: 0.24rem;
    top: 50%;
    margin-top: -0.325rem;
}

/*rank*/
[m-role="UISegment"] ul.rank-tab {
    margin: 0.2rem 0.3rem;
    border-radius: 5px;
    border: 1px solid #157fd0;
    height: 0.8rem;
}

[m-role="UISegment"] ul.rank-tab:after {
    display: none;
}

[m-role="UISegment"] ul.rank-tab li[m-active='yes']:after {
    display: none;
}

[m-role="UISegment"] ul.rank-tab li {
    color: #157fd0;
}

[m-role="UISegment"] ul.rank-tab li[m-active='yes'] {
    background: #157fd0;
    color: #fff;
}

.rank-pic > img {
    height: 2.1rem;
}

/*recommend*/
[m-role='UITableView'][m-style='grouped'] > dl.recommend > dd {
    padding: 0 0.8rem 0 0;
}

/*fund-information*/
.tab-fund {
    height: 0.9rem;
    position: relative;
}

.tabs {
    height: 0.9rem;
    width: 100%;
    background: #fff;
    position: fixed;
    z-index: 51;
}

.tabs:after {
    position: absolute;
    bottom: 0;
    left: 0rem;
    right: 0rem;
    content: ' ';
    width: auto;
    height: 1px;
    border-bottom: 1px solid #e6eaed;
    transform: scaleY(0.5);
!important;
}

.tab {
    position: relative;
    color: #999;
    font-size: 0.3rem;
}

.tab:before {
    position: absolute;
    bottom: 0.3rem;
    top: 0.3rem;
    right: 0rem;
    content: ' ';
    width: 1px;
    height: auto;
    border-right: 1px solid #e6eaed;
    transform: scaleX(0.5);
!important;
}

.tab:last-child:before {
    display: none;
}

.tab.up-active {
    color: #157fd0;
}

.tab.up-active:after {
    width: 100%;
    overflow: hidden;
    content: ' 20';
    display: block;
    height: 0.04rem;
    clear: both;
    background: #157fd0;
    position: absolute;
    bottom: 1px;
    left: 0;
}

.tab-contents {
    width: 100%;
    display: none;
}

.asset-icon {
    display: block;
    width: 0.2rem;
    height: 0.34rem;
    background: url("images/asset-icon.png") no-repeat;
    background-size: 0.5rem 0.34rem;
}

.asset-icon.asset-left {
    margin-left: 0.3rem;
    background-position: left center;
}

.asset-icon.asset-right {
    margin-right: 0.3rem;
    background-position: right center;
}

[m-role='UITableView'][m-style='grouped'] > dl.tab-news > dd {
    padding: 0 0.8rem 0 0.35rem;
    min-height: 1.3rem;
    position: relative;
    border: none;
}

[m-role='UITableView'][m-style='grouped'] > dl.tab-news > dd:after {
    position: absolute;
    bottom: 0;
    left: 0rem;
    right: 0rem;
    content: ' ';
    width: auto;
    height: 1px;
    border-bottom: 1px solid #e6eaed;
    transform: scaleY(0.5);
!important;
}

/*search*/
.search-field {
    height: 0.9rem;
    padding: 0 0.3rem;
}

.search-field [m-role='UISearchBar'] .UISearchBarRoom.UISearchBarLayoutFixed {
    position: relative;
    border-bottom: none;
    height: 0.6rem;
    background: none;
    top: 0.15rem;
    left: 0rem;
    right: 0rem;
    z-index: 50;
}

.search-field [m-role='UISearchBar'] {
    background: #f2f5f7;
    position: relative;
    height: 0.9rem;
}

.search-field [m-role='UITextField'] [m-icon='search'] {
    width: 0.35rem;
    height: 0.4rem;
    background: url("images/search-icon.png") center center no-repeat;
    background-size: 100%;
    margin-top: 0.1rem;
    margin-left: 0.1rem;
}

.search-field [m-role='UITextField'] {
    border-radius: 8px;
}

.search-field [m-role='UITextField'] input {
    font-size: 0.3rem;
    color: #999;
}

.search-banner {
    height: 2.6rem !important;
}

.search-img {
    width: 100%;
    height: 2.6rem;
}

.hot-search {
    min-height: 5rem;
    padding: 0 0.3rem;
}

.hot-search div {
    display: block;
    float: left;
    padding: 0 0.2rem;
    height: 0.65rem;
    line-height: 0.65rem;
    text-align: center;
    border: 1px solid #777777;
    font-size: 0.28rem;
    color: #333;
    border-radius: 5px;
    margin: 0.2rem 0.2rem 0rem 0;
}

.icon-s {
    width: 0.4rem;
    height: 0.4rem;
    background: url(images/histroy-icon.png) no-repeat;
    background-size: 100%;
    display: block;
}

.icon-clear {
    width: 0.28rem;
    height: 0.4rem;
    background: url(images/clear.png) no-repeat;
    background-size: 100%;
    display: block;
}

.find-fund {
    height: 0.65rem;
    background: #fff;
    font-size: 0.28rem;
    position: relative;
    padding-left: 0.4rem;
}

.find-fund:after {
    position: absolute;
    bottom: 0;
    left: 0rem;
    right: 0rem;
    content: ' ';
    width: auto;
    height: 1px;
    border-bottom: 1px solid #e6eaed;
    transform: scaleY(0.5);
!important;
}

.news-type {
    border: 1px solid #999;
    border-radius: 4px;
    padding: 0.04rem 0.15rem;
    margin-right: 0.5rem;
    font-size: 0.24rem;
}

/*链接*/
.h-link {
    min-height: 0.9rem;
    position: absolute;
    left: 0;
    right: 0;
    padding: 0 0.8rem 0 0.35rem;
    z-index: 10;
}

/*light-asset*/
[m-role='UITableView'][m-style='grouped'] > dl.asset-news {
    -webkit-box-shadow: none;
}

[m-role='UITableView'][m-style='grouped'] > dl.asset-news > dd {
    padding: 0 0.3rem 0 0.35rem;
    min-height: 0.9rem;
    position: relative;
    border: none;
}

[m-role='UITableView'][m-style='grouped'] > dl.asset-news > dd:after {
    position: absolute;
    bottom: 0;
    left: 0rem;
    right: 0rem;
    content: ' ';
    width: auto;
    height: 1px;
    border-bottom: 1px solid #e6eaed;
    transform: scaleY(0.5);
!important;
}

.outline-bj {
    background: #fff;
    padding: 0.3rem;
}

.num-table {
    width: 100%;
    background: #fff;
}

.num-table > thead > tr {
    height: 0.6rem;
    color: #4da5f3;
}

.num-table > tbody > tr {
    height: 0.9rem;
    color: #333;
}

.num-table > thead > tr > th,
.num-table > tbody > tr > td {
    position: relative;
}

.num-table > thead > tr > th:after,
.num-table > tbody > tr > td:after {
    position: absolute;
    bottom: 0;
    left: 0rem;
    right: 0rem;
    content: ' ';
    width: auto;
    height: 1px;
    border-bottom: 1px solid #e6eaed;
    transform: scaleY(0.5);
!important;
}

.num-table > tfoot > tr {
    height: 0.9rem;
    color: #333;
}

.num-table > thead > tr > th:nth-child(2),
.num-table > tbody > tr > td:nth-child(2),
.num-table > tfoot > tr > td:nth-child(2) {
    width: 2.5rem;
}

.num-table > tbody > tr > td:nth-child(1),
.num-table > thead > tr > th:nth-child(1),
.num-table > tfoot > tr > td:nth-child(1) {
    width: 2.2rem;
    padding-left: 0.3rem;
}

.num-table > tbody > tr > td:nth-child(3),
.num-table > thead > tr > th:nth-child(3),
.num-table > tfoot > tr > td:nth-child(3) {
    width: 2.2rem;
    padding-right: 0.3rem;
}

.num-table > tbody > tr > td:nth-child(4),
.num-table > thead > tr > th:nth-child(4),
.num-table > tfoot > tr > td:nth-child(4) {
    width: 2.2rem;
    padding-right: 0.3rem;
}
/*fund-infor*/
.chart-tab {
    background: #fff;
}

.fundNum {
    min-height: 2.5rem;
    background: #fd6749;
    padding-bottom: 0.2rem;
}

.fund-border {
    border: 1px solid #fff;
    border-radius: 4px;
    padding: 0.04rem 0.2rem;
    color: #fff;
}

[m-role="UISegment"] ul.fund-tab-one,
[m-role="UISegment"] ul.fund-tab-two {
    height: 0.9rem;
    position: relative;
}

[m-role="UISegment"] ul.fund-tab-one:after,
[m-role="UISegment"] ul.fund-tab-two:after {
    position: absolute;
    bottom: 0;
    left: 0rem;
    right: 0rem;
    content: ' ';
    width: auto;
    height: 1px;
    border-bottom: 1px solid #e6eaed;
    transform: scaleY(0.5);
!important;
}

[m-role="UISegment"] ul.fund-tab-one li,
[m-role="UISegment"] ul.fund-tab-two li {
    color: #999;
    font-size: 0.3rem;
    position: relative;
}

[m-role="UISegment"] ul.fund-tab-one li:nth-child(1):before,
[m-role="UISegment"] ul.fund-tab-two li:nth-child(1):before {
    position: absolute;
    bottom: 0.1rem;
    top: 0.1rem;
    right: 0rem;
    content: ' ';
    width: 1px;
    height: auto;
    border-right: 1px solid #e6eaed;
    transform: scaleX(0.5);
!important;
}

[m-role="UISegment"] ul.fund-tab-one li[m-active='yes'],
[m-role="UISegment"] ul.fund-tab-two li[m-active='yes'] {
    color: #157fd0;
}

[m-role="UISegment"] ul.fund-tab-one li[m-active='yes']:after,
[m-role="UISegment"] ul.fund-tab-two li[m-active='yes']:after {
    width: 100%;
    overflow: hidden;
    content: ' 20';
    display: block;
    height: 0.04rem;
    clear: both;
    background: #157fd0;
    position: absolute;
    bottom: 1px;
    left: 0;
    border-bottom: none;
    transform: scaleY(1);
}

[m-role="UISegment"] ul.fund-time {
    height: 0.6rem;
    position: relative;
    padding: 0.45rem 0.6rem 0.3rem;
}

[m-role="UISegment"] ul.fund-time:after {
    display: none;
}

[m-role="UISegment"] ul.fund-time li {
    color: #333;
    font-size: 0.2rem;
    position: relative;
    border: 1px solid #e6eaed;
    margin-left: -1px;
}

[m-role="UISegment"] ul.fund-time li:nth-child(1) {
    margin-left: 0;
}

[m-role="UISegment"] ul.fund-time li[m-active='yes'] {
    color: #fd6749;
    border-color: #fd6749;
}

[m-role="UISegment"] ul.fund-time li[m-active='yes'] + li {
    margin-left: 0px;
}

[m-role="UISegment"] ul.fund-time li[m-active='yes']:after {
    display: none;
}

[m-role='UITableView'][m-style='grouped'] > dl.news {
    -webkit-box-shadow: none;
}

[m-role='UIButton'].bottom-fix {
    width: 3.35rem;
    margin: 0 0.1rem;
    height: 0.8rem;
    color: #fff;
    font-size: 0.34rem;
    border-radius: 5px;
}

[m-role='UIButton'].bottom-fix.dt {
    background: #157fd0;
}

[m-role='UIButton'].bottom-fix.sg {
    background: #fd6749;
}

/*manager*/
.w350 {
    width: 3.5rem;
}

.manager-infor {
    min-height: 2.2rem;
    background: #fff;
    padding: 0 0.3rem;
}

.manager-per {
    margin: 0 0.2rem;
    background-size: 100%;
    width: 1.5rem;
    height: 1.5rem;
    border-radius: 50%;
}

.manager-per img {
    width: 1.5rem;
