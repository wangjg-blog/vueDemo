/*进度图*/
.circle {
    width: 0.7rem;
    height: 0.7rem;
    position: absolute;
    border-radius: 50%;
    /*background: #4da5f3;*/
    top:50%;
    margin-top: -0.35rem;
    left: 50%;
    margin-left: -0.35rem;
}
.pie_left, .pie_right {
    width: 0.7rem;
    height: 0.7rem;
    position: absolute;
    top: 0;left: 0;
}
.left, .right {
    display: block;
    width:0.7rem;
    height:0.7rem;
    background:#999999;
    border-radius: 50%;
    position: absolute;
    top: 0;
    left: 0;
}
.pie_right, .right {
    clip:rect(0,auto,auto,0.35rem);
}
.pie_left, .left {
    clip:rect(0,0.35rem,auto,0);
}
.mask-c {
    width: 0.6rem;
    height: 0.6rem;
    border-radius: 50%;
    left: 50%;
    top: 50%;
    margin-top: -0.3rem;
    margin-left: -0.3rem;
    background: #FFF;
    position: absolute;
    text-align: center;
    line-height: 0.6rem;
    font-size: 0.24rem;
    color: #4da5f3;
}


/*swiper*/
.swiper-container1 {
    height: 0.9rem;
    width: 100%;
    background: #fff;
    z-index: 51;
}
.swiper-container1 .swiper-slide {
    text-align: left;
}
.bar {
    position: absolute;
    height: 2px;
    bottom: 0;
    overflow: hidden;
    -webkit-box-sizing: border-box;
    padding: 0 10px;
    -webkit-transition: left 200ms ease-out;
}
.tab-slide{
    line-height: 0.9rem;
    text-align: center;
    font-size: 0.24rem;
}
.swiper-slide .title {
    text-align: center;
}
.tab.tab-slide.up-active:after{
    bottom: 0!important;
}

/*右上角弹出菜单*/

#topPopover {
    position: fixed;
    bottom: 1rem;
    right: 0.05rem;
     }
#topPopover .mui-popover-arrow {
    left: 50%;
    margin-left: -10px;
}

.mui-popover {
    height: 1.8rem; }

.mui-content {
    padding: 0px;
}
.mui-popover {
    position:fixed;
    /*display: none;*/
    width: 1.9rem;
    -webkit-transition: opacity .3s;
    transition: opacity .3s;
    -webkit-transition-property: opacity;
    transition-property: opacity;
    -webkit-transform: none;
    transform: none;
    /* opacity: 0; */
    background-color: #fff;
    -webkit-box-shadow: 0 0 10px rgba(0, 0, 0, .1);
    box-shadow: 0 0 10px rgba(0, 0, 0, .1)
}

.mui-popover .mui-popover-arrow {
    position: absolute;
    bottom: -0.2rem;
    left: 0;
    overflow: hidden;
    width:20px;
    height:20px;
}

.mui-popover .mui-popover-arrow:after {
    position: absolute;
    top: -0.15rem;
    left: 0;
    width: 20px;
    height: 20px;
    content: ' ';
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
    border-radius: 0.03rem;
    background: #fff
}
.mui-scroll-wrapper {
    position: absolute;
    z-index: 2;
    top: 0;
    bottom: 0;
    left: 0;
    overflow: hidden;
    width: 100%
}
.mui-popover .mui-scroll-wrapper {
    border-radius: 7px;
    background-clip: padding-box
}
.mui-scroll {
    position: absolute;
    z-index: 1;
    width: 100%;
    -webkit-transform: translateZ(0);
    transform: translateZ(0)
}
.mui-table-view{
    width: auto;
    height: 100%;
    overflow: hidden;
}
.mui-table-view li{
    height: 0.9rem;
    line-height: 0.9rem;
    text-align: center;
    position: relative;

}
.mui-table-view li:after{
    overflow: hidden;
    content: ' 20';
    display: block;
    height: 1px;
    clear: both;
    background: #eee;
    position: absolute;
    top: -1px;
    left: 0;
    right: 0;
}
.mui-table-view li a:active{
    color: #e92635;
}
